[flake8]
exclude =
    .git,
    __pycache__,
    venv,
    .rasa,
    .vscode

filename = *.py
accept-encodings = utf-8
inline-quotes = single
max-complexity = 12
max-linenumber = 500
max-line-length = 119
multiline-quotes = double

per-file-ignores = __init__.py:F401,
                   dash_app.py:F401
